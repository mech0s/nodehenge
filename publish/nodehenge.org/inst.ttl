tool:alertingAndNotification a ont:AbstractTool ;
    skos:definition """Improve visibility of system events
Reduce system downtime
Improve customer service""" ;
    skos:editorialNote "InputsText and OutputText source fields need cleansed and turned into related Atrifact entities" ;
    skos:prefLabel "Alerting and notification" ;
    skos:scopeNote """Notify security teams and/or administrators about detected events.
Support automatic remediation of high-priority time-critical events.""" .

tool:assetInventoryManagement a ont:AbstractTool ;
    skos:definition "Increase situation awareness" ;
    skos:editorialNote "InputsText and OutputText source fields need cleansed and turned into related Atrifact entities" ;
    skos:prefLabel "Asset inventory management" ;
    skos:scopeNote "Maintain a “real-time” inventory of all applications, software licenses, libraries, operating systems, and versioning information" .

tool:complianceAsCode a ont:AbstractTool ;
    skos:definition "Automated detection and, ideally, remediation of non-compliance within the infrastructure" ;
    skos:editorialNote "InputsText and OutputText source fields need cleansed and turned into related Atrifact entities" ;
    skos:prefLabel "Compliance as Code" ;
    skos:scopeNote "Monitor the state of compliance of deployed COTS against STIGs" .

tool:configurationManagementDatabaseCmdb a ont:AbstractTool ;
    skos:definition "Centralized database used by many systems (such as asset management, configuration management, incident management, etc.) during development and operations phases." ;
    skos:editorialNote "InputsText and OutputText source fields need cleansed and turned into related Atrifact entities" ;
    skos:prefLabel "Configuration management database (CMDB)" ;
    skos:scopeNote """Auto-discovery;
Dependency mapping;
Integration with other tools;
Configuration auditing""" .

tool:configurationManagementTool a ont:AbstractTool ;
