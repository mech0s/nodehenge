@base <http://nodehenge.org/ont>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.

<#AbstractTool> a rdfs:Class.
<#SpecifiedTool> a rdfs:Class.


_:exampleAbstractTool a <#AbstractTool>, skos:Concept;
    skos:prefLabel "Example Abstract Tool"
.
_:exampleSpecifiedTool a <#SpecifiedTool>, skos:Concept;
    skos:prefLabel "Example Specified Tool";
    skos:broader _:exampleAbstractTool
.
_:ExampleDeployedTool a rdfs:Class, skos:Concept;
    skos:prefLabel "Example Deployed Tool";
    skos:broader _:exampleSpecifiedTool
.
_:exampleToolExecution a _:ExampleDeployedTool, skos:Concept;
    skos:prefLabel "Example Tool Execution"
.





